<% if(!pstStatus.includes(pstValue)){ %>
    <li class="single-seat">
        <img src="../assets/images/movie/seat01.png" alt="seat">
    </li>
<% }  
   else { 
    let i = pstStatus.indexOf(pstValue)%>
    <li class="single-seat seat-free" onclick="getSeat('<%=pstValue%>','<%=prices[i]%>')">
        <img src="../assets/images/movie/seat01-free.png" alt="seat" id="s<%=pstValue%>">
        <span class="sit-num"><%=pstValue%></span>
    </li>
<% } %>